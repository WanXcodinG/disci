{
  "name": "disci",
  "version": "0.0.1",
  "description": "A Http Request handler for discord interactions",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.mjs",
    "types": "./dist/index.d.ts"
  },
  "license": "Apache-2.0",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src/**/*.ts --fix",
    "prettier:check": "prettier --parser=typescript \"src/**/*.{js,ts}\" --check",
    "prettier:write": "prettier --parser=typescript \"src/**/*.{js,ts}\" --write"
  },
  "dependencies": {
    "discord-api-types": "^0.37.22",
    "tslib": "^2.4.1",
    "undici": "^5.20.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.15",
    "eslint": "^8.34.0",
    "tsup": "^6.6.0"
  },
  "keywords": [
    "discord",
    "interactions",
    "discordapp",
    "slash-commands",
    "slash-command",
    "api",
    "discord-interactions"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/typicalninja493/disci.git"
  },
  "bugs": {
    "url": "https://github.com/typicalninja493/disci/issues"
  },
  "homepage": "https://disci.typical.gq/"

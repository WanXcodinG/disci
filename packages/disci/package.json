{
  "name": "disci",
  "version": "1.0.0",
  "description": "A Http Request handler for discord interactions",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.mjs",
    "types": "./dist/index.d.ts"
  },
  "license": "Apache-2.0",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "dependencies": {
    "discord-api-types": "^0.37.22",
    "prettier": "^2.8.4",
    "tslib": "^2.4.1"
  },
  "devDependencies": {
    "@types/node": "^18.11.15",
    "eslint": "^8.34.0",
    "eslint-config-custom": "workspace:*",
    "tsconfig": "workspace:*",
    "tsup": "^6.6.0"
  },
  "keywords": [
    "discord",
    "interactions",
    "discordapp",
    "slash-commands",
    "slash-command",
    "api",
    "discord-interactions"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/typicalninja493/disci.git"
  },
  "bugs": {
    "url": "https://github.com/typicalninja493/disci/issues"
  }
}

{
  "name": "@discijs/fastify-handler",
  "version": "1.0.0",
  "description": "DisciJS support for fastify based webservers",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.mjs",
    "types": "./dist/index.d.ts"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prettier:check": "prettier --parser=typescript \"src/**/*.{js,ts}\" --check",
    "prettier:write": "prettier --parser=typescript \"src/**/*.{js,ts}\" --write",
    "lint:check": "eslint \"src/**/*.{js,ts}\"",
    "lint": "eslint",
    "lint-staged": "lint-staged"
  },
  "author": "",
  "license": "Apache-2.0",
  "keywords": [
    "discord",
    "interactions",
    "discordapp",
    "slash-commands",
    "slash-command",
    "api",
    "discord-interactions",
    "fastify"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/typicalninja493/disci.git"
  },
  "bugs": {
    "url": "https://github.com/typicalninja493/disci/issues"
  },
  "homepage": "https://disci.typical.gq/",
  "lint-staged": {
    "*.{js,ts}": [
      "pnpm lint"
    ],
    "*.{json,js,ts,jsx,tsx,html}": [
      "pnpm prettier:write"
    ]
  },
  "devDependencies": {
    "tsup": "^6.6.0"
  },
  "dependencies": {
    "fastify": "^4.10.2",
    "disci": "workspace:^"
  }
}

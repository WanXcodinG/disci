{
	"root": true,
	"extends": [
		"turbo", 
		"prettier",
		"plugin:@typescript-eslint/recommended"
	],
	"parser": "@typescript-eslint/parser",
	"plugins": [
		"@typescript-eslint"
	],
	"rules": {
		"@typescript-eslint/consistent-type-definitions": ["error", "interface"],
		"turbo/no-undeclared-env-vars": "off"
	},
	"ignorePatterns": ["**/dist/*"]
}
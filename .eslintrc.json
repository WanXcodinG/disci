{
	"root": true,
	"extends": [
		"plugin:@typescript-eslint/recommended",
		"prettier"
	],
	"parser": "@typescript-eslint/parser",
	"plugins": [
		"@typescript-eslint"
	],
	"rules": {
		"@typescript-eslint/consistent-type-definitions": ["error", "interface"],
		"key-spacing":  "error"
	},
	"ignorePatterns": ["**/dist/*"]
}